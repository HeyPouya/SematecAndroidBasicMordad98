<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sematec.sematecandroidbasicmordad98">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TestViewPagerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TestFragmentActivity"></activity>
        <activity android:name=".TestNotificationActivity"></activity>
        <activity android:name=".TestCustomTextActivity" />
        <activity android:name=".TestServiceActivity" />
        <activity android:name=".TestWebViewActivity" />
        <activity
            android:name=".TestDialogActivity"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity android:name=".TestAlertDialogActivity" />
        <activity android:name=".TestHandlerActivity" />
        <activity android:name=".TestMenuActivity" />
        <activity android:name=".TestPermissionActivity" />
        <activity android:name=".TestSelectorActivity" />
        <activity android:name=".TestVideoViewActivity" />
        <activity android:name=".TestDBActivity" />
        <activity android:name=".TestAsyncClientActivity" />
        <activity android:name=".TestURLConnectionActivity" />
        <activity android:name=".TestThreadActivity" />
        <activity android:name=".TestHTMLActivity" />
        <activity android:name=".TestRecyclerActivity" />
        <activity android:name=".TestNavigationDrawerActivity" />
        <activity android:name=".TestSharedPrefrencesActivity" />
        <activity android:name=".TestIntentActivity" />
        <activity android:name=".TestActivity" />
        <activity android:name=".MainActivity" />

        <receiver android:name=".TestBroadcastReceiver">
            <intent-filter>
                <action android:name="android.intent.action.AIRPLANE_MODE" />
            </intent-filter>
        </receiver>

        <service android:name=".TestService" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.CAMERA" />

</manifest>